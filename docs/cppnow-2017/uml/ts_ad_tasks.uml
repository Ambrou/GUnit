@startuml

start

:Initialize Decision Engine;

note right
[1.0] Parse Feeds
end note

:Connect to the Exchange;

note right
[1.0] Parse Feeds
end note

note right
[1.0] Parse Feeds
end note

while (There are Feeds?) is (yes)

    :Process Feeds;
    note right
        [1.1] Sell shares
    end note

    if (Desired shares were sold/bougth?) then (bought)
        if (There is enoguh to buy shares?) then (yes)
            :Get Shares to buy;
        endif
    else (sold)
        :Get shares to sell;
        note right
            [1.1] Sell shares
        end note
    endif
    :Execeute order!;
        note right
        [1.1] Sell shares
    end note

endwhile (no)
note right
[1.0] Parse Feeds
end note


:Disconnect from the Exchange;
note right
[1.0] Parse Feeds
end note


stop

@enduml

