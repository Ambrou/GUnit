@startuml

start

:Connect to the MarketData and the Exchange;

:Process Feeds;

while (There are new feeds?) is (yes)
    note right
      [0.0.2] Handle buy
    endnote
    if (Subscribed stock is rising?) then (yes)
        :Get Shares to buy;
        note right
          [0.0.2] Handle buy
        endnote
    else (no)
        :Get shares to sell;
    endif
    :Execute order;
      note right
        [0.0.2] Handle buy
      endnote
endwhile (no)

:Disconnect;

stop

@enduml
